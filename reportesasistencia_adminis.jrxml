	<queryString>
		<![CDATA[SELECT
     asp."idasistencia" AS ID,
     c."descripcion" AS Cargo,
     p."nombre" AS Nombre,
     p."fapellido" AS FApellido,
     p."lapellido" AS LApellido,
     p.dni AS DNI,
     asp."fecha" AS Fecha,
     asp."hora_ingreso" AS Hora_Ingreso,
     asp."hora_salida" AS Hora_Salida
FROM
     "asistencia_personal" asp INNER JOIN "personal" p ON asp."dni" = p."dni"
     INNER JOIN "cargo" c ON p."idcargo" = c."idcargo"
WHERE
     p.idpersonal = 3
 AND asp.estado = 'A'
ORDER BY
     nombre ASC]]>
	</queryString>
